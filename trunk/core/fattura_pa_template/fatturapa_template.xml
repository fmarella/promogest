<?xml version="1.0" encoding="UTF-8"?>
<p:FatturaElettronica versione="1.0"
xmlns:ds="http://www.w3.org/2000/09/xmldsig#"
xmlns:p="http://www.fatturapa.gov.it/sdi/fatturapa/v1.0"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <FatturaElettronicaHeader>
    <DatiTrasmissione>
      <IdTrasmittente>
        <IdPaese>IT</IdPaese>
        <IdCodice>{{trasmittente.codice}}</IdCodice>
      </IdTrasmittente>
      <ProgressivoInvio>{{trasmissione.progressivo_invio}}</ProgressivoInvio>
      <FormatoTrasmissione>{{trasmissione.formato_trasmissione}}</FormatoTrasmissione>
      <CodiceDestinatario>{{trasmissione.codice_destinatario}}</CodiceDestinatario>
    </DatiTrasmissione>
    <CedentePrestatore>
      <DatiAnagrafici>
        <IdFiscaleIVA>
          <IdPaese>IT</IdPaese>
          <IdCodice>{{prestatore.codice}}</IdCodice>
        </IdFiscaleIVA>
        <Anagrafica>
          <Denominazione>{{prestatore.denominazione}}</Denominazione>
        </Anagrafica>
        <RegimeFiscale>{{prestatore.codice_regime_fiscale}}</RegimeFiscale>
      </DatiAnagrafici>
      <Sede>
		<Indirizzo>{{prestatore.sede_indirizzo}}</Indirizzo>
		<CAP>{{prestatore.sede_cap}}</CAP>
		<Comune>{{prestatore.sede_comune}}</Comune>
		<Provincia>{{prestatore.sede_provincia}}</Provincia>
		<Nazione>{{prestatore.sede_nazione}}</Nazione>
      </Sede>
      <StabileOrganizzazione>
		<Indirizzo>{{organizzazione.stabile_indirizzo}}</Indirizzo>
		<CAP>{{organizzazione.stabile_cap}}</CAP>
		<Comune>{{organizzazione.stabile_comune}}</Comune>
		<Provincia>{{organizzazione.stabile_provincia}}</Provincia>
		<Nazione>{{organizzazione.stabile_nazione}}</Nazione>
      </StabileOrganizzazione>
    </CedentePrestatore>
    <CessionarioCommittente>
      <DatiAnagrafici>
        <CodiceFiscale>{{committente.codice_fiscale}}</CodiceFiscale>
        <Anagrafica>
          <Denominazione>{{committente.denominazione}}</Denominazione>
        </Anagrafica>
      </DatiAnagrafici>
      <Sede>
        <Indirizzo>{{committente.sede_indirizzo}}</Indirizzo>
		<CAP>{{committente.sede_cap}}</CAP>
		<Comune>{{committente.sede_comune}}</Comune>
		<Provincia>{{committente.sede_provincia}}</Provincia>
		<Nazione>{{committente.sede_nazione}}</Nazione>
      </Sede>
    </CessionarioCommittente>
    <SoggettoEmittente>CC</SoggettoEmittente>
  </FatturaElettronicaHeader>
	<FatturaElettronicaBody>
		<DatiGenerali>
			<DatiGeneraliDocumento>
				<TipoDocumento>TD01</TipoDocumento>
				<Divisa>EUR</Divisa>
				<Data>2012-11-27</Data>
				<Numero>00001</Numero>
				<Art73>SI</Art73>
			</DatiGeneraliDocumento>
			<DatiOrdineAcquisto>
				<RiferimentoNumeroLinea>1</RiferimentoNumeroLinea>
				<IdDocumento>123</IdDocumento>
				<CodiceCUP>123abc</CodiceCUP>
				<CodiceCIG>456def</CodiceCIG>
			</DatiOrdineAcquisto>
			<DatiContratto>
				<RiferimentoNumeroLinea>1</RiferimentoNumeroLinea>
				<IdDocumento>123</IdDocumento>
				<Data>2012-09-01</Data>
				<NumItem>5</NumItem>
				<CodiceCUP>123abc</CodiceCUP>
				<CodiceCIG>456def</CodiceCIG>
			</DatiContratto>
			<DatiConvenzione>
				<RiferimentoNumeroLinea>1</RiferimentoNumeroLinea>
				<IdDocumento>123</IdDocumento>
				<Data>2012-09-01</Data>
				<NumItem>5</NumItem>
				<CodiceCUP>123abc</CodiceCUP>
				<CodiceCIG>456def</CodiceCIG>
			</DatiConvenzione>
			<DatiRicezione>
				<RiferimentoNumeroLinea>1</RiferimentoNumeroLinea>
				<IdDocumento>123</IdDocumento>
				<Data>2012-09-01</Data>
				<NumItem>5</NumItem>
				<CodiceCUP>123abc</CodiceCUP>
				<CodiceCIG>456def</CodiceCIG>
			</DatiRicezione>
			<DatiTrasporto>
				<DatiAnagraficiVettore>
					<IdFiscaleIVA>
						<IdPaese>IT</IdPaese>
						<IdCodice>{{dao.codice_vettore}}</IdCodice>
					</IdFiscaleIVA>
					<Anagrafica>
						<Denominazione>{{dao.ragione_sociale_vettore}}</Denominazione>
					</Anagrafica>
				</DatiAnagraficiVettore>
				<DataOraConsegna>{{utils.dateTimeToString(dao.fine_trasporto)}}</DataOraConsegna>
			</DatiTrasporto>
		</DatiGenerali>
		<DatiBeniServizi>
            {% for riga in dao.righe %}<DettaglioLinee>
				<NumeroLinea>{{loop.index}}</NumeroLinea>
				<Descrizione>{{riga.descrizione|nonone}} {{riga.aggiuntalottoindescrizione}}</Descrizione>
				<PrezzoUnitario>{{utils.mNLC(riga.valore_unitario_lordo,2)}}</PrezzoUnitario>
				<PrezzoTotale>{{utils.mNLC(( riga.totaleRiga ),2)}}</PrezzoTotale>
				<AliquotaIVA>{{riga.percentuale_iva|int}}</AliquotaIVA>
			</DettaglioLinee>{% endfor %}
            {% for iva in riepilogo_iva %}<DatiRiepilogo>
				<AliquotaIVA>{{iva.totale_aliquota}}</AliquotaIVA>
				<ImponibileImporto>{{iva.imponibile_iva}}</ImponibileImporto>
				<Imposta>{{iva.imposta}}</Imposta>
			</DatiRiepilogo>{% endfor %}
		</DatiBeniServizi>
		 <DatiPagamento>
      <CondizioniPagamento>{{dao.condizioni_pagamento}}</CondizioniPagamento>
      {% for scadenza in dao.scadenze %}<DettaglioPagamento>
        <ModalitaPagamento>{{ scadenza.pagamento }}</ModalitaPagamento>
        <DataScadenzaPagamento>{{ scadenza.data_pagamento|dateformat }}</DataScadenzaPagamento>
        <ImportoPagamento>{{ utils.mNLC(scadenza.importo, 2) }}</ImportoPagamento>
    </DettaglioPagamento>{% endfor %}
      </DatiPagamento>
  </FatturaElettronicaBody>
</p:FatturaElettronica>
