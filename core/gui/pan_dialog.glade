  <object class="GtkImage" id="acquista_image">
    <property name="visible">True</property>
    <property name="pixbuf">carte16x16.png</property>
  </object>
  <object class="GtkImage" id="regstrati_image">
    <property name="visible">True</property>
    <property name="stock">gtk-edit</property>
  </object>
  <object class="GtkImage" id="attiva_image">
    <property name="visible">True</property>
    <property name="stock">gtk-apply</property>
  </object>
  <object class="GtkImage" id="preventivo_image">
    <property name="visible">True</property>
    <property name="stock">gtk-edit</property>
  </object>
</interface>
