<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkWindow" id="main_window">
    <property name="title" translatable="yes">PromoGest2</property>
    <property name="window_position">center</property>
    <property name="icon">promogest.png</property>
    <property name="type_hint">dialog</property>
    <signal name="destroy" handler="on_main_window_destroy"/>
    <signal name="key_press_event" handler="on_main_window_key_press_event"/>
    <signal name="close" handler="on_main_window_destroy"/>
    <signal name="window_state_event" handler="on_main_window_window_state_event"/>
    <child>
      <object class="GtkVBox" id="dialog_vbox">
        <property name="visible">True</property>
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkMenuBar" id="main_menubar">
            <property name="visible">True</property>
            <child>
              <object class="GtkMenuItem" id="file_menu">
                <property name="visible">True</property>
                <property name="label" translatable="yes">_File</property>
                <property name="use_underline">True</property>
              </object>
            </child>
            <child>
              <object class="GtkMenuItem" id="opzioni_menu">
                <property name="visible">True</property>
                <property name="label" translatable="yes">_Opzioni</property>
                <property name="use_underline">True</property>
                <child type="submenu">
                  <object class="GtkMenu" id="opzioni_menu_menu">
