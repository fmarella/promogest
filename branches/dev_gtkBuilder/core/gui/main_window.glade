                        <property name="label" translatable="yes">Importa Modulo</property>
                        <property name="use_underline">True</property>
                        <signal name="activate" handler="on_importa_modulo_activate"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkMenuItem" id="dati_azienda">
                        <property name="visible">True</property>
                        <property name="tooltip_text" translatable="yes">i dati della azienda</property>
