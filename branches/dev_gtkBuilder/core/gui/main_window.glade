                      <object class="GtkMenuItem" id="licenza_menu">
                        <property name="visible">True</property>
                        <property name="label" translatable="yes">Vedi la licenza</property>
                        <property name="use_underline">True</property>
                        <signal name="activate" handler="on_licenza_menu_activate"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkMenuItem" id="seriale_menu">
                        <property name="visible">True</property>
                        <property name="label" translatable="yes">Codice installazione...</property>
                        <property name="use_underline">True</property>
                        <signal name="activate" handler="on_seriale_menu_activate"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkMenuItem" id="manuale_online">
                        <property name="visible">True</property>
                        <property name="label" translatable="yes">Manuale online</property>
                        <property name="use_underline">True</property>
                        <signal name="activate" handler="on_manuale_online_activate"/>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
