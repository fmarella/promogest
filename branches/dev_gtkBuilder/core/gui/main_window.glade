                        <property name="visible">True</property>
                        <property name="label" translatable="yes">server</property>
                        <property name="use_underline">True</property>
                        <signal name="activate" handler="on_master_sincro_db_activate"/>
                      </object>
                    </child>
                    <child>
                      <object class="GtkMenuItem" id="client_sincro_db">
                        <property name="visible">True</property>
                        <property name="label" translatable="yes">client</property>
                        <property name="use_underline">True</property>
                        <signal name="activate" handler="on_client_sincro_db_activate"/>
                      </object>
